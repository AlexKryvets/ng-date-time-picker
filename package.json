        "lint-staged": "^13.0.3",
        "ng-packagr": "^11.0.0",
        "prettier": "^2.7.1",
        "protractor": "~7.0.0",
        "ts-node": "~8.3.0",
        "tslint": "~6.1.0",
        "typescript": "~4.1.5"
